{
    "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "moduleResolution": "Node",
      "baseUrl": "src",
      "outDir": "./dist",
      "esModuleInterop": true,
      "strict": true
    },
    "ts-node": {
      "esm": true
    },
    "include": [
        "*/.ts", "./src/*/"
    ],
    "exclude": [
        "node_modules"
    ]
  }
